CFLAGS	Makefile	/^CFLAGS	:=	-g -Wall -O2 -mword-relocations \\$/;"	m
CPPFILES	Makefile	/^CPPFILES	:=	$(foreach dir,$(SOURCES),$(notdir $(wildcard $(dir)\/*.cpp)))$/;"	m
CXXFLAGS	Makefile	/^CXXFLAGS	:= $(CFLAGS) -fno-rtti -fno-exceptions -std=gnu++11$/;"	m
DATA	Makefile	/^DATA		:=	data$/;"	m
DEPSDIR	Makefile	/^export DEPSDIR	:=	$(CURDIR)\/$(BUILD)$/;"	m
GFXBUILD	Makefile	/^GFXBUILD	:=	$(BUILD)$/;"	m
